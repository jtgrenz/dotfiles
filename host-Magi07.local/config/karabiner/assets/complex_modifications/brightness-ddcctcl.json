{
  "title": "Brightness external monitor control",
  "rules": [
    {
      "manipulators": [
        {
          "description": "brightness down",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/bin/sh /Users/jongrenning/Development/ddcctl/ddcctl.sh down"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "manipulators": [
        {
          "description": "brightness up",
          "from": {
            "key_code": "f2",
            "modifiers": {
              "mandatory": [
                "left_alt"
              ]
            }
          },
          "to": [
            {
              "shell_command": "/bin/sh /Users/jongrenning/Development/ddcctl/ddcctcl.sh up"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
